{
  "name": "web.sveltekit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development vite dev",
    "dev:emulator": "NODE_ENV=development VITE_FIREBASE_EMULATOR=true vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "stripe:listen": "stripe listen --events checkout.session.completed --forward-to localhost:5173/payment/webhooks",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@iconify/json": "^2.2.164",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@zerodevx/svelte-toast": "^0.9.5",
    "autoprefixer": "^10.4.16",
    "daisyui": "latest",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "unplugin-icons": "^0.18.1",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "@octokit/auth-app": "^6.0.3",
    "@octokit/core": "^5.1.0",
    "firebase": "^10.7.1",
    "nanoid": "^5.0.4",
    "stripe": "^14.12.0",
    "theme-change": "^2.5.0"
  }
}
