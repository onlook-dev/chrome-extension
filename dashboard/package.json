{
	"name": "web.sveltekit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "NODE_ENV=development vite dev",
		"dev:emulator": "NODE_ENV=development VITE_FIREBASE_EMULATOR=true vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"stripe:listen": "stripe listen --events checkout.session.completed --forward-to localhost:5173/payment/webhooks",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
