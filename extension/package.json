{
  "name": "onlook.dev",
  "displayName": "Onlook",
  "description": "Inspect, test, and edit any web app or website online.",
  "version": "1.4.3",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --watch src/lib/editor/bundle.min.js --exec 'vite build --watch'",
    "dev:emulator": "NODE_ENV=development VITE_FIREBASE_EMULATOR=true nodemon --watch src/lib/editor/bundle.min.js --exec 'vite build --watch'",
    "build": "NODE_ENV=production vite build && bun run badlinks && bun run zip",
    "build:links": "NODE_ENV=production vite build && bun run zip",
    "badlinks": "bun scripts/badlinks.ts -r dist",
    "zip": "zip -r dist.zip dist",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.18",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@tsconfig/svelte": "5.0.2",
    "@types/chrome": "0.0.243",
    "@types/mixpanel-browser": "^2.49.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "autoprefixer": "^10.4.14",
    "commander": "^12.1.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-svelte": "^2.35.0",
    "hot-reload-extension-vite": "^1.0.13",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.2",
    "svelte-check": "3.5.0",
    "svelte-preprocess": "5.0.4",
    "tailwindcss": "^3.3.2",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "typescript-eslint-parser": "^22.0.0",
    "vite": "4.2.3"
  },
  "dependencies": {
    "@extend-chrome/messages": "^1.2.2",
    "@extend-chrome/storage": "^1.5.0",
    "@langchain/community": "^0.2.9",
    "@langchain/langgraph": "^0.0.22",
    "@langchain/openai": "^0.1.2",
    "@skeletonlabs/skeleton": "^2.8.0",
    "@types/valid-url": "^1.0.7",
    "firebase": "^10.7.1",
    "highlight.js": "^11.9.0",
    "html-to-image": "^1.11.11",
    "mixpanel": "^0.18.0",
    "mixpanel-browser": "juanfer0002/mixpanel-js-mv3#fetch-support-for-mv3-extensions",
    "nanoid": "^5.0.3",
    "rxjs": "^7.8.1",
    "valid-url": "^1.0.9",
    "webext-bridge": "^6.0.1"
  }
}